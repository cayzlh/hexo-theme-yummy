<section class="jumbotron geopattern" data-pattern-id="<%- page.title %>">
    <div class="container">
        <div id="jumbotron-meta-info">
            <h1><%= page.title %></h1>
            <span class="meta-info">
                <a><span class="octicon octicon-calendar"></span> <%- date(page.date, "YYYY-MM-DD") %></a>
                <% if(page.categories) { %>
                    <a><span class="octicon octicon-book"></span></a>
                    <% page.categories.forEach(function (cate) { %>
                        <a href="<%- url_for(cate.path) %>"><%= cate.name %></a>
                    <% }); %>
                <%}%>
                <% if (theme.busuanzi.enable) { %>
                    <% if (theme.busuanzi.post.pv) { %>
                        <a>
                            <span class="octicon octicon-eye-watch"
                                  id="busuanzi_value_page_pv"><i class="fa fa-spinner fa-spin"></i></span>
                        </a>
                    <% } %>
                <% } %>
            </span>
        </div>

    </div>
</section>
<script>
    $(document).ready(function(){

        $('.geopattern').each(function(){
            $(this).geopattern($(this).data('pattern-id'));
        });

    });
</script>
<section class="jumbotron geopattern" data-pattern-id="All articles are here">
    <div class="container">
        <div id="jumbotron-meta-info">
            <h1><%= page.title %></h1>
            <span class="meta-info">
                    <span class="mega-octicon octicon-tag"></span>
                    <%= page.subtitle %>
            </span>
        </div>
    </div>
</section>
<script>
    $(document).ready(function () {

        $('.geopattern').each(function () {
            $(this).geopattern($(this).data('pattern-id'));
        });

    });
</script>
<article class="post container">
    <ul class="archives-list">
        <%
        var tags = [];

        site.tags.each(function (tag) {
            tags.push(tag);
        });
        %>
        <% tags.forEach(function (tag) { %>
            <h3>
                <a style="color: black;" href="<%- url_for(tag.path) %>" target="_blank"><%= tag.name %></a>
            </h3>
            <% tag.posts.forEach(function (p) { %>
                <li>
                    <a href="<%- url_for(p.path) %>" target="_blank"><%= p.title %></a>
                </li>
            <% }); %>
        <% }); %>
    </ul>
</article>
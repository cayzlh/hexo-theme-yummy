<div class="pagination text-align">
    <div class="btn-group">
        <% if(page.prev) { %>
            <a href="<%- url_for(page.prev_link) %>" class="btn btn-outline">&laquo;</a>
        <% } else { %>
            <button disabled="disabled" href="javascript:;" class="btn btn-outline">&laquo;</button>
        <% } %>

        <% if (page.total) { %>
            <% for (var i = 1; i <= page.total; i++) { %>
                <% if (i == 1) { %>
                    <a href="<%- url_for() %>" class="<% if (i==page.current) { %>active<% } %> btn btn-outline"><%- i %></a>
                <% } else { %>
                    <a href="<%- url_for("/page/" + i) %>" class="<% if (i==page.current) { %>active<% } %> btn btn-outline"><%- i %></a>
                <% } %>
            <% } %>
        <% } %>

        <% if(page.next) { %>
            <a href="<%- url_for(page.next_link) %>" class="btn btn-outline">&raquo;</a>
        <% } else { %>
            <button disabled="disabled" href="javascript:;" class="btn btn-outline">&raquo;</button>
        <% } %>
    </div>
</div>


